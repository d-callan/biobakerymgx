nextflow_process {

    name "Test Process HUMANN_HUMANN"
    script "../main.nf"
    process "HUMANN_HUMANN"
    config "./nextflow.config"

    tag "modules"
    tag "modules_nfcore"
    tag "humann"
    tag "humann/humann"

    test("DEMO_diamond") {
        when {
            process {
                """
                input[0] = "DEMO_diamond"
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }
    }

    test("DEMO_diamond - stub") {

        options "-stub"

        when {
            process {
                """
                input[0] = "DEMO_diamond"
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }
    }
}
